# tab centric:

exporter
{
    events
    {
        { count: x, timestamp: ..., userid: abc }
        { count: x, timestamp: ..., userid: abc }
        { count: x, timestamp: ..., userid: abc }
        ...
    }
}

toolbox
{
    events
    {
        { toolid: 123, timestamp: ..., userid: abc }
        { toolid: 123, timestamp: ..., userid: abc }
        { toolid: 123, timestamp: ..., userid: abc }
        ...
    }
}

