# event centric

events
{
	{ timestamp: ttt, user: userIndex, tool: toolIndex, export_count: 1 }
    { timestamp: ttt, user: userIndex, tool: toolIndex, export_count: 1 }
    { timestamp: ttt, user: userIndex, tool: toolIndex }
    ...
}

lists
{
    users: ['Anil', 'Trilok', 'haX0r', ...]
    tools: ['saveas', 'deletemetadata', 'export', ... ]
}

